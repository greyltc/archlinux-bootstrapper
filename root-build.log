  gpg --trust-model always --no-default-keyring --keyring ./vendors.gpg --verify *.sig
  rm *.sig

  sudo tar xzf archlinux-bootstrap-*-x86_64.tar.gz
  sudo mv root.x86_64 root

  popd
}

